<%- include('../partials/header') %>
<body>
  <img id="user-image" src="<%= user.avatar %>" alt="Avatar" />
  <h3><%= user.name %></h3>
  <% plants.forEach(p => { %>
  <img id="plant-image" src="<%= p.image %>" /><br />
  <a href="plants/<%= p._id %>"><%= p.commonName %></a><br />
  <% }) %>
</body>
